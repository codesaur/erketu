{
    "name": "codesaur/erketu",
    "description": "An elegant object-oriented application development framework for PHP 7.2 or newer",
    "keywords": [
        "erketu",
        "mongol", "mongolia", "mongolian", "монгол",
        "codesaur", "narankhuu", "coden", "кодзавр", "наранхүү"," коден",
        "ololt", "ололт", "мөнхийн", "мөнхийн-ололт", "мөнхийн ололт", "munkhiin ololt"
    ],
    "homepage": "https://ololt.mn/codesaur",
    "authors": [
        {
            "name": "Narankhuu",
            "email": "codesaur@gmail.com",
            "homepage": "https://ololt.mn/narankhuu"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",        
        "psr/log": ">=1.1.3",
        "psr/container": ">=1.0.0",
        "psr/http-message": ">=1.0.1",
        "psr/http-server-middleware": ">=1.0.1",
        "twig/twig": ">=3.2.1",
        "vlucas/phpdotenv": ">=5.2.0",
        "fig/http-message-util": ">=1.1.5"
    },
    "autoload": {
        "psr-4": {
            "codesaur\\Globals\\": "src/globals/",
            "codesaur\\Http\\": "src/http/",
            "codesaur\\Http\\Message\\": "src/http/message/",
            "codesaur\\Http\\Router\\": "src/http/router/",
            "codesaur\\Template\\": "src/template/",
            "codesaur\\DataObject\\": "src/dataobject/"
        },
        "files": [
            "src/codesaur.php"
        ]
    },
    "license": "MIT",
    "minimum-stability": "dev"
}
